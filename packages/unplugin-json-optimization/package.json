{
  "name": "@uni_toolkit/unplugin-json-optimization",
  "type": "module",
  "version": "0.0.12",
  "description": "A plugin to optimize json files generation",
  "license": "MIT",
  "homepage": "https://github.com/uni-toolkit/uni-toolkit/tree/main/packages/unplugin-json-optimization",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-toolkit/uni-toolkit.git",
    "directory": "packages/unplugin-json-optimization"
  },
  "bugs": {
    "url": "https://github.com/uni-toolkit/uni-toolkit/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "transform",
    "json",
    "optimization"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@rollup/pluginutils": "catalog:",
    "unplugin": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^22.15.21",
    "nodemon": "^3.1.10",
    "rollup": "^4.41.0",
    "tsup": "catalog:",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "webpack": "^5.99.9"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "tsx src/index.ts"
  }
}